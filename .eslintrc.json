{
    "extends": ["airbnb","prettier","plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error",{
            "endOfLine": "auto"}
          ],
        "no-console":"off",
        "spaced-comment":"off",
        "consistent-return":"off",
        "func-names":"off",
        "prefer-arrow-callback":"off",
        "prefer-destructuring":"off",
        "no-undef":"off"
       
    }
}
